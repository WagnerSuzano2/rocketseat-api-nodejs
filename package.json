{
  "name": "rocketseat-api-nodejs",
  "version": "1.0.0",
  "description": "Repositório do projeto \"Sua Primeira API com Node.js\" da Rocketseat. Uma API RESTful construída com Node.js, TypeScript e Express, com integração de banco de dados PostgreSQL via Docker, testes automatizados e documentação com Swagger",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node-dev --env-file .env --respawn --transpile-only src/server.ts",
    "db:seed": "ts-node-dev --env-file .env src/database/seed.ts",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "pretest": "dotenv -e .env.test drizzle-kit migrate",
    "test": "dotenv -e .env.test vitest run --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WagnerSuzano2/rocketseat-api-nodejs.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/WagnerSuzano2/rocketseat-api-nodejs/issues"
  },
  "homepage": "https://github.com/WagnerSuzano2/rocketseat-api-nodejs#readme",
  "dependencies": {
    "@fastify/swagger": "^9.5.1",
    "@fastify/swagger-ui": "^5.2.3",
    "@scalar/fastify-api-reference": "^1.34.6",
    "drizzle-orm": "^0.44.4",
    "fastify": "^5.5.0",
    "fastify-type-provider-zod": "^5.0.3",
    "pg": "^8.16.3",
    "pino-pretty": "^13.1.1",
    "zod": "^4.1.3"
  },
  "devDependencies": {
    "@faker-js/faker": "^10.0.0",
    "@types/node": "^24.2.1",
    "@types/supertest": "^6.0.3",
    "@vitest/coverage-v8": "^3.2.4",
    "dotenv-cli": "^10.0.0",
    "drizzle-kit": "^0.31.4",
    "supertest": "^7.1.4",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  }
}
